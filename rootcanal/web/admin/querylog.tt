[% PROCESS header.tt title="STEDT Database: Query Log" %]
[% self_url = c.query.url('-absolute',1) -%]
[% self_base = self_url.replace('[^/]+$' , '') -%]
<body>
<p>[% queries.size %] most recent queries:</p>

<table>
<tr>
	<th>table</th>
	<th>query</th>
	<th>ip</th>
	<th>time</th>
</tr>
[% FOREACH row IN queries -%]
<tr>
[% FOREACH col IN row -%]
<td>[% col %]</td>
[% END -%]
</tr>
[% END -%]
</table>

</body>
</html>
